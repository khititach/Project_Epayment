<h3 class="text-center"> --------- category show food test v2 ---------</h3>
<div class="row">
    <div class="col" >
        <h3 class="text-center card card-body ml-5">Store NO : 1</h3>
    </div>
    <div class="col">
        <h3 class="text-center card card-body mr-5">Store Name : Khaw man kai</h3>
    </div>
</div>
<div class="card card-body ml-3 mr-3">
    <!-- food no i -->
    <div class="row ">
        <table class="table table-bordered" >
            <thead>
                <tr>
                    <td width='30%'>Name</td>
                    <td width='10%'>Type</td>
                    <td width='10%'>Extra</td>
                    <td width='20%'>Price</td>
                    <td width='10%'>Calorise</td>
                    <td width='10%'>Shotcut</td>
                    <td width='10%'>Edit</td>
                </tr>
            </thead>
            <tbody>
                <% for (var i = 0; i < categoryData.length; i++) { %>
                    <tr>
                        <td rowspan="2"><p><%= categoryData[i].food_name %></p></td>
                        <td rowspan="2"><%= categoryData[i].food_type %></td>
                        <td>Normal</td>
                        <td><%= categoryData[i].food_normal.price %></td>
                        <td rowspan="2"><%= categoryData[i].food_calories %></td>
                        <td><%= categoryData[i].food_normal.shotcut %></td>
